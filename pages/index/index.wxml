<!--pages/index/index.wxml-->
<swiper class="sliders">
  <swiper-item wx:for="{{mySliders}}" wx:key="id">
    <image  src="{{item.image}}" mode="aspectFill"></image>
  </swiper-item>

</swiper>
<view class='grids'>
  <navigator url="/pages/list/list?cat={{item.id}}"class="item" wx:for="{{myCategories}}" wx:key="id">
    <image src="{{item.icon}}"></image>
    <text>{{item.name}}</text>
  </navigator>


</view>
  <view class="links">
  <image src='/assets/link-01.png'></image>
  <image src='/assets/link-02.png'></image>

  </view>